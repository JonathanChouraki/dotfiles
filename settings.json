{
    "editor.lineNumbers": "relative",
    "vim.easymotion": true,
    "vim.foldfix": true,
    "vim.leader": ",",
    "vim.substituteGlobalFlag": true,
    "vim.useSystemClipboard": true,
    "vim.visualstar": true,
    "keyboard.dispatch": "keyCode",
    "workbench.colorTheme": "Palenight Theme",
    "editor.minimap.enabled": false,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "g",
                "h"
            ],
            "commands": [
                "workbench.action.navigateLeft"
            ]
        },
        {
            "before": [
                "g",
                "l"
            ],
            "commands": [
                "workbench.action.navigateRight"
            ]
        },
        {
            "before": [
                "<tab>"
            ],
            "commands": [
                "editor.action.jumpToBracket"
            ]
        },
        {
            "before": [
                "<leader>",
                "s"
            ],
            "commands": [
                "workbench.action.splitEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "<space>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "<C-b>"
            ],
            "commands": [
                "workbench.action.toggleSidebarVisibility"
            ]
        }
    ],
    "vim.sneak": true,
    "vim.sneakUseIgnorecaseAndSmartcase": true,
    "vim.hlsearch": true,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "files.trimTrailingWhitespace": true,
    "explorer.confirmDragAndDrop": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "elm.analyseEnabled": true,
    "[elm]": {
        "editor.formatOnSave": true
    },
    "window.titleBarStyle": "custom",
    "liveshare.featureSet": "insiders",
    "editor.tabSize": 2,
    "diffEditor.ignoreTrimWhitespace": false,
    "files.associations": {
        "*.js": "javascript"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "vim.camelCaseMotion.enable": true,
    "vim.changeWordIncludesWhitespace": true,
    "editor.fontFamily": "'FuraMono Nerd Font', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "explorer.confirmDelete": false,
    "emmet.includeLanguages": {
        "twig": "html",
        "javascriptreact": "html",
        "javascript": "html",
    },
    "workbench.editor.enablePreview": false,
    "editor.fontWeight": "700",
    "oneDarkPro.editorTheme": "Onedark Pro",
    "go.autocompleteUnimportedPackages": true,
    "window.zoomLevel": 0,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "ghcid.command": "/home/box/.local/bin/ghcid",
    "haskell.hlint.executablePath": "/home/box/.local/bin/hlint",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "go.formatTool": "goimports",
    "go.useLanguageServer": true,
    "vim.enableNeovim": true,
    "vscode-neovim.mouseSelectionStartVisualMode": true,
    "vim.neovimPath": "/usr/bin/nvim",
    "vscode-neovim.neovimPath": "/usr/bin/nvim",
    "vscode-neovim.neovimInitPath": "/home/box/.config/nvim/init.vim",
    "autoimport.higherOrderComponents": "connect|withRouter|withUser",
    "purescript.addNpmPath": true,
    "purescript.addSpagoSources": true,
    "purescript.buildCommand": "/home/box/.npm-global/bin/spago build --purs-args --json-errors",
    "editor.suggestSelection": "first",
    "editor.fontSize": 16,
    "editor.formatOnType": true,
    "editor.formatOnPaste": true,
    "sass.format.setPropertySpace": false,
    "eslint.packageManager": "yarn",
    "eslint.format.enable": true,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": false
}
